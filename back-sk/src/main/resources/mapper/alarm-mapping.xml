<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.curation.dao.alarm.AlarmDao">

    <!-- 상품명에 해당하는 상품번호 찾아오기 -->
    <select id="getAlarmByEmail" parameterType="string" resultType="com.web.curation.model.vo.Alarm">
        select * from alarm
        where person1 = #{value}
    </select>
</mapper>