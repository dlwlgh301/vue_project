<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.curation.dao.user.UserDao">

    <!-- 상품명에 해당하는 상품번호 찾아오기 -->
    <select id="getProductNumberByProductName" parameterType="string" resultType="com.web.curation.model.board.Board">
        select productNumber from product
        where productName = "공주거울"
    </select>
</mapper>