<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.curation.dao.product.ProductDao">
   <insert id="insertProduct" parameterType="com.web.curation.model.vo.Product" >
        insert into product(productName, link, image, price)
        values(#{productName}, #{link}, #{image}, #{price})
    </insert>
</mapper>